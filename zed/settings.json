{
  "assistant": {
    "default_model": {
      "provider": "openai",
      "model": "google/gemini-2.0-pro-exp-02-05:free"
    },
    "version": "2"
  },
  "language_models": {
    "openai": {
      "api_url": "https://openrouter.ai/api/v1",
      "available_models": [
        {
          "name": "google/gemini-2.0-pro-exp-02-05:free",
          "display_name": "gemini-2.0",
          "max_tokens": 2000000
        }
      ],
      "version": "1"
    }
  },
  "auto_install_extensions": {
    "beancount": false,
    "nix": false
  },
  "autosave": "on_focus_change",
  "base_keymap": "VSCode",
  "buffer_font_family": "Menlo",
  "buffer_line_height": "comfortable",
  "cursor_blink": false,
  "features": {
    "copilot": false,
    "inline_completion_provider": "none"
  },
  "format_on_save": "off",
  "lsp": {
    "nil": {
      "settings": {
        "formatting": {
          "command": ["alejandra", "--"]
        }
      }
    }
  },
  // "proxy": "socks5://127.0.0.1:6153",
  "relative_line_numbers": true,
  "remove_trailing_whitespace_on_save": false,
  "restore_on_startup": "none",
  "scrollbar": {
    "show": "never"
  },
  "tab_size": 2,
  "terminal": {
    "copy_on_select": true,
    "option_as_meta": true,
    "toolbar": {
      "title": false
    }
  },
  "vim": {
    "use_smartcase_find": true,
    "use_system_clipboard": "always"
  },
  "vim_mode": true
}
